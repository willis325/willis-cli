{
  "name": "willis-cli",
  "type": "module",
  "version": "1.1.0",
  "description": "常用功能脚手架工具，支持克隆模板项目仓库、更新git仓库 master 主干分支、docker 镜像构建与重启等",
  "author": "willis<793207918@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/willis325/willis-cli",
  "bugs": "https://github.com/willis325/willis-cli/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/willis325/willis-cli.git"
  },
  "bin": {
    "willis-cli": "bin/index.js"
  },
  "main": "bin/index.js",
  "files": [
    "bin",
    "README",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint --fix .",
    "prettier": "prettier --write .",
    "prepublishOnly": "npm run build",
    "prepare": "npx simple-git-hooks"
  },
  "keywords": [
    "willis",
    "willis-cli",
    "cli",
    "inquirer",
    "commander",
    "download-git-repo",
    "chalk",
    "ora",
    "simple-git-hooks",
    "typescript",
    "tsup"
  ],
  "simple-git-hooks": {
    "pre-commit": "npm run lint && npm run prettier"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "download-git-repo": "^3.0.2",
    "inquirer": "^12.0.0",
    "ora": "^8.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.7.6",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "eslint": "^8.57.1",
    "prettier": "^3.3.3",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3"
  }
}
